  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Has signed the informed consent form (patient or parent/guardian)."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Male or female of any race and ethnicity."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Patient is two years of age or older with AD, or a history of EH, or patient is 18 years of age or older with asthma, psoriasis, or is a normal healthy control."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Patient falls into one of the following diagnostic categories as defined by the ADVN. Standardized Diagnostic Criteria: ? Normal healthy control patient with no systemic disorders as outlined in the exclusion criteria ? Patient with a history of active AD ? Patient with a history of quiescent AD ? Patient with a history of psoriasis ? Patient with a history of asthma not requiring systemic medications. Medical records must document standard American Thoracic Society (ATS) criteria, i.e., a history of reversible wheezing or improvement in forced expiratory volume (FEV)1 by at least 12% following use of a bronchodilator ? Patient with a history of EH."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Patient who is pregnant or lactating."
          ]
        },
