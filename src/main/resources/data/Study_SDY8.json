  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Sign the informed consent form (or a parent or guardian, if applicable)."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "1 to 65 years of age."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Residing in the U.S."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "No systemic disorders (i.e., immunodeficiency disorders such as human immunodeficiency virus [HIV] or lupus erythematosus) other than the condition being studied."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Meet the criteria for one of the diagnostic groups (ADEH+, ADEH-, or NA; see Appendix I), as confirmed by an ADVN investigator or co-investigator.  The standardized definitions (see Appendix I) for ADEH+, ADEH-, and NA Control have been developed for use in all ADVN studies and will be used to determine eligibility."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "ADEH- and NA Control subjects must be HSV 1 or 2 antibody positive."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Subjects must be enrolled in the ADVN Biomarker Registry Study prior to the CMI-HSV Substudy."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Unable or unwilling to complete the informed consent process."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Has atopy but lacks stringent AD features (only presumptive diagnosis of AD)."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "History of any systemic illness other than the condition being studied."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Has autoimmune or immunodeficiency disease.  Presence of active systemic malignancy, excluding uncomplicated non-melanoma skin cancer. "
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Presence of any skin disease other than AD that might compromise the stratum corneum barrier (e.g., bullous disease, psoriasis, cutaneous T cell lymphoma [also called Mycosis Fungoides or Sezary syndrome])."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Receiving an experimental new drug as part of a separate clinical trial.  If it is a Food and Drug Administration (FDA) approved drug the subject will be allowed to enroll and the information will be captured in their medication history."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects receiving systemic immunosuppressives, oral corticosteroids, or immunomodulatory medicaion within 12 days of enrollment."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects receiving immunotherapy within the last year."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects using topical medications including (but not restricted to) Elidel, Protopic, topical corticosteroids, or topical antibiotics at the site of the blood draw within the last 72 hours."
          ]
        }
  ],
  
