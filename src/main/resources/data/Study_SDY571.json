  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Patients age >= 10 years AND >= 30kg"
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Individuals on the waiting list for a transplant for whom a donor heart has become available"
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Subject and/or parent guardian able to understand and provide informed consent"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Women of childbearing potential with a positive serum or urine pregnancy test or nursing mothers. Women of childbearing potential not using 2 reliable forms of contraception, including a barrier method, for the duration of study"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Inability or unwillingness of a participant to give written informed consent or comply with study protocol"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Recipients of multiple organ transplants"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Previous history of organ transplantation"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Known significant donor coronary artery disease defined by left main coronary artery obstruction >=50%, or single major epicardial coronary artery >=75%, or 2 or more major epicardial coronary arteries with >=50% obstruction as defined by coronary angiography"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Other co-morbidities determined by the site investigator"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Currently taking immunosuppression for non-transplant reasons"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Participation in an interventional investigational drug clinical trial"
          ]
        }
