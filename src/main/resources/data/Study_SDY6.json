  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Sign the informed consent form (or a parent or guardian, if applicable)."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "8 months to 80 years of age."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Self reported Ethnicity and Race as either Non Hispanic African American or Non Hispanic Caucasian for the ADEH- and NA healthy control groups."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Residing in the U.S."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "No systemic disorders (i.e., immunodeficiency disorders such as human immunodeficiency virus [HIV] or lupus erythematosus) other than the condition being studied."
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "Meet the criteria for one of the diagnostic groups (see Appendix I), as confirmed by an ADVN investigator or co-investigator.  The standardized definitions (see Appendix I) for ADEH+, ADEH-, ADMC, NAMC, ADEV+, ADEV-, and NA Control have been developed for use in all ADVN studies and will be used to determine eligibility."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Unable or unwilling to complete the informed consent process."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects that self report as more than one race or a race other than African American or Caucasian for the ADEH- or NA group."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Subjects that self report as Hispanic for the ADEH- or NA group."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Has atopy but lacks stringent AD features (only presumptive diagnosis of AD)."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "History of any systemic illness other than the condition being studied."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Has autoimmune or immunodeficiency disease. "
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Presence of active systemic malignancy, excluding uncomplicated non-melanoma skin cancer."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Presence of any skin disease other than AD that might compromise the stratum corneum barrier (e.g., bullous disease, psoriasis, cutaneous T cell lymphoma [also called Mycosis Fungoides or Sezary syndrome])."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Receiving an experimental new drug as part of a separate clinical trial.  If it is a Food and Drug Administration (FDA) approved drug the subject will be allowed to enroll and the information will be captured in their medication history."
          ]
        }
  ],
  
