  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Have had Type 1 diabetes mellitus for more than 5 years and exhibit one of the following:  hypoglycemic unawareness, metabolic instability with2 or more episodes of sever hypoglycemia, 2 or more hospital visits for diabetic ketoacidosis in last year, retinopathy, nephropathy, neuropathy"
          ]
        },
        {
          "category": "Inclusion",
          "values": [
              "age 18-65 years of age"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have had severe cardiac disease"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "actively abuse alcohol or substances including cigarette smoking"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have psychiatric problems preventing them from being suitable candidate"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have history of not following prescribed regimens"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have active infection including hepatitis C virus, hepatitis B virus, human immunodeficiency virus (HIV) or Tuberculosis"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have a history of malignancy, except squamous or basal skin cell cancer"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "weigh more than 70kilograms or have a Body Mass Index greater than 26 kilograms per meter squared."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have C-peptide valud of 0.3 ng per mL or more following 5.0 gram intravenous arginine infusion challenge"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "unable to provide informed consent"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have gallstones or hemangioma in liver"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have untreated prolferative retinopathy"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "are breastfeeding, pregnant or intend to try to become pregnant or to father a child or fail to follow birth control methods"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have had previous transplant, or evidence of anti-human leukocyte antigen (HLA) antibody"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have an insulin requirement of more than 0.7 International Units per kilogram per day"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have a blood glycosylated hemoglobin (HbAic) higher than 12 percent"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "are unable to reach the hospital for transplantation within 2 hours of notification"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have untreated or treated hyperlipidemia"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have a medical condition requiring chronic use of steroids"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "use coumadin or other anticoagulants"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have Addison's disease"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "have a negative screen for Epstein-Barr virus"
          ]
        }
  ],
  
