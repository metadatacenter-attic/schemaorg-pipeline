  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Genrally healthy and ambulatory at time of initial study enrollment"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "History of immunodeficiency or any known or suspected impairment of immunologic function at time of initial study enrollment"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Recent or current use of immunosuppressive medication at the time of initial study enrollment"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Acute systemic or serious concurrent illness on Day 0"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Receipt of a vaccination up to 8 weeks prior to enrollment on Day 0"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Recipients or donors of blood or blood products within the past 6 months or 6 weeks, respectively, prior to enrollment at Day 0"
