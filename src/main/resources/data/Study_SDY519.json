  "selectionCriteria": [
        {
          "category": "Inclusion",
          "values": [
              "Genrally healthy and ambulatory children and adults"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Prior vaccination with TIV or LAIV in Fall 2011"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Allergy to egg or egg products, or to vaccine components, including thimerosal (TIV multidose vials only)."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Allergy to latex (for Group F only - may be assigned to Fluzone High-Dose). Review with investigator to determine if this would affect the volunteer?s safety."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Life-threatening reactions to previous influenza vaccinations"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Active systemic or serious concurrent illness, including febrile illness on the day of vaccination"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "History of immunodeficiency"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Known or suspected impairment of immunologic function, including, but not limited to, clinically significant liver disease, diabetes mellitus treated with insulin, moderate to severe renal disease, or any other chronic disorder which, in the opinion of the investigator, might jeopardize volunteer safety or compliance with the protocol."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Blood pressure >150/95 at V01"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Hospitalization in the past year for congestive heart failure or emphysema."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Chronic Hepatitis B or C."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Recent or current use of immunosuppressive medication, including glucocorticoids (corticosteroid nasal sprays are permissible). Use of oral steroids (<20mg predisone-equivalent/day) may be acceptable after review by the investigator."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Malignancy, other than squamous cell or basal cell skin cancer (includes solid tumors such as breast cancer or prostate cancer with recurrence in the past year, and any hematologic cancer such as leukemia)."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Autoimmune disease (including rheumatoid arthritis treated with immunosuppressive medication such as Plaquenil, methotrexate, prednisone, Enbrel) which, in the opinion of the investigator, might jeopardize volunteer safety or compliance with the protocol."
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "History of blood dyscrasias, renal disease, or hemoglobinopathies requiring regular medical follow up or hospitalization during the preceding year"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Use of any anti-coagulation medication such as Coumadin or Lovenox, of anti-platelet agents such as aspirin (except aspirin up to 325 mg.day), Plavix, or Aggrenox must be reviewed by investigator to determine if this would affect the volunteer?s safety. Plavix, Aggrenox must be reviewed by investigator"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Receipt of blood or blood products within the past 6 months"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Medical or psychiatric condition or occupational responsibilities that preclude subject compliance with the protocol"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Inactivated vaccine 14 days prior to vaccination"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Live, attenuated vaccine within 60 days of vaccination"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "History of Guillain-Barr_ Syndrome"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Pregnant or lactating woman"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Use of investigational agents within 30 days prior to enrollment"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Donation of the equivalent of a unit of blood within 6 weeks prior to enrollment"
          ]
        },
        {
          "category": "Exclusion",
          "values": [
              "Any condition which, in the opinion of the investigator, might interfere with volunteer safety, study objectives or the ability of the participant to understand or comply with the study protocol."
          ]
        }
