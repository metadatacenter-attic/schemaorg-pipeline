{
  "@context":"http://schema.org",
  "@type":"MedicalStudy",
  "name":.title,
  "description":.description,
  "population":[.producedBy.selectionCriteria[]? | .values[] + " (" + .category + ")"],
  "sponsor":[.acknowledges[]? | {
    "@type":"Organization",
    "name":.name
  }],
  "url":("https://schemaorg.metadatacenter.org/immport/Study_" + (.identifier.identifier | split("/")[8]) + ".html"),
  "sameAs":[.distributions[].access.accessURL]
}